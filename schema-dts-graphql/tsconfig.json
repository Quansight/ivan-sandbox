{
  "compilerOptions": {
    "declaration": true,
    "jsx": "react",
    "lib": [
      "es2015",
      "dom",
      "esnext.asynciterable"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "rootDir": "src",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "es2015",
    "types": [],
    "composite": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
